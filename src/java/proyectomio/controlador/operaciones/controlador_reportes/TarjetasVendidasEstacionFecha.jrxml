	<queryString language="SQL">
		<![CDATA[SELECT
     estacion.`id_estacion` AS estacion_id_estacion,
     estacion.`nombre` AS estacion_nombre,
     estacion.`direccion` AS estacion_direccion,
     estacion.`id_empleado_a_cargo` AS estacion_id_empleado_a_cargo,
     count(tarjeta.`id_tarjeta`) * 3200 AS tarjeta_id_tarjeta,
     tarjeta.`estado` AS tarjeta_estado,
     tarjeta.`saldo` AS tarjeta_saldo,
     tarjeta.`id_estacion_venta` AS tarjeta_id_estacion_venta,
     tarjeta.`fecha_venta` AS tarjeta_fecha_venta
FROM
     `estacion` estacion INNER JOIN `tarjeta` tarjeta ON estacion.`id_estacion` = tarjeta.`id_estacion_venta`
WHERE
    'tarjeta_fecha_venta' BETWEEN 'FD01' AND 'FD02'
GROUP BY
    'estacion_id_estacion'
]]>
	</queryString>
